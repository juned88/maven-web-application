node('NODES')
{
def mavenhome = tool name: "maven3.8.6"
stage('git fetch')
{
git branch: 'development', credentialsId: 'e981d4ed-aa30-4ae5-aaa3-dd283a9cfdb2', url: 'https://github.com/juned88/maven-web-application.git'
}
stage('package building')
{
sh "${mavenhome}/bin/mvn clean package"
}
stage('sonar report')
{
    sh "${mavenhome}/bin/mvn sonar:sonar"
}
stage('upload to repo')
{
    sh "${mavenhome}/bin/mvn deploy"
}
}//node closing
